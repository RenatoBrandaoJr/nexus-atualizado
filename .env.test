# Configurações de ambiente de teste para o Nexus

# Configurações gerais
NODE_ENV=test
PORT=3000
LOG_LEVEL=info

# Configurações do banco de dados
DB_HOST=test-db.example.com
DB_PORT=5432
DB_NAME=nexus_test
DB_USER=nexus_test
DB_PASSWORD=test_password_secure

# Configurações do TaskMaster
TASKMASTER_ENABLED=true
TASKMASTER_PATH=/app
TASKMASTER_DEFAULT_STATUS=pending
TASKMASTER_DEFAULT_PRIORITY=medium
TASKMASTER_STATUS_COLUMN_MAP={"pending":"A fazer","in-progress":"Em andamento","review":"Revisão","done":"Concluído","deferred":"Adiado","cancelled":"Cancelado"}

# Configurações de API
API_KEY=test_key_secure_789012
API_TIMEOUT=30000

# Configurações de autenticação
JWT_SECRET=test_jwt_secret_key_secure
JWT_EXPIRATION=1d

# Configurações de deploy
TEST_DEPLOY_HOST=test-server.example.com
TEST_DEPLOY_USER=deploy
TEST_DEPLOY_PATH=/var/www/nexus
