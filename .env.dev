# Configurações de ambiente de desenvolvimento para o Nexus

# Configurações gerais
NODE_ENV=development
PORT=3000
LOG_LEVEL=debug

# Configurações do banco de dados
DB_HOST=localhost
DB_PORT=5432
DB_NAME=nexus_dev
DB_USER=postgres
DB_PASSWORD=postgres

# Configurações do TaskMaster
TASKMASTER_ENABLED=true
TASKMASTER_PATH=/Users/renatobrandao/Documents/Nexus atualizado
TASKMASTER_DEFAULT_STATUS=pending
TASKMASTER_DEFAULT_PRIORITY=medium
TASKMASTER_STATUS_COLUMN_MAP={"pending":"A fazer","in-progress":"Em andamento","review":"Revisão","done":"Concluído","deferred":"Adiado","cancelled":"Cancelado"}

# Configurações de API
API_KEY=dev_key_123456
API_TIMEOUT=30000

# Configurações de autenticação
JWT_SECRET=dev_jwt_secret_key
JWT_EXPIRATION=1d
